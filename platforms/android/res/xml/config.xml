    <gap:splash gap:platform="android" gap:qualifier="port-ldpi" src="www/res/screen/ios/5c-portrait.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-mdpi" src="www/res/screen/ios/5c-portrait.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-hdpi" src="www/res/screen/ios/5c-portrait.png" />
